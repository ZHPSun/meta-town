datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model User {
  id          String  @id @default(dbgenerated()) @map("id")
  displayName String 
  avatar      String
  createdAt   DateTime @default(now()) @map("created_at")

  @@map("users")
}
